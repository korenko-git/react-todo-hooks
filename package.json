{
  "name": "react-todo-hooks",
  "version": "1.0.0",
  "description": "Awesome Todo app",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config ./webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js",
    "analyze": "cross-env BUNDLE_ANALYZER=true npm run build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . && echo \"No lint errors!\" && exit 0",
    "lint:js:fix": "eslint . --fix && echo \"Lint errors fixed!\" && exit 0",
    "lint:css": "stylelint ./src/**/*.js && echo \"No lint errors!\" && exit 0",
    "lint:staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3",
    "moment": "^2.29.1",
    "polished": "^4.0.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-datetime": "^3.0.4",
    "react-dom": "^17.0.1",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-react-constant-elements": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@svgr/webpack": "5.5.0",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/jest": "^26.0.19",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-terminal-webpack-plugin": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "dotenv": "8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "favicons-webpack-plugin": "5.0.0-alpha.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "5.0.0-alpha.17",
    "husky": "^4.3.6",
    "ip": "^1.1.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "style-loader": "2.0.0",
    "stylelint": "13.8.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "url-loader": "^4.1.1",
    "webpack": "5.10.0",
    "webpack-bundle-analyzer": "4.2.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.4.1",
    "webpackbar": "5.0.0-3",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "browserslist": [
    "last 2 versions",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": [
      "npm run lint:js:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  }
}
